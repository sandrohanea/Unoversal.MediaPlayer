<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
					xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:ump="using:Unoversal.MediaPlayer"
                     mc:Ignorable="d">

    <Style TargetType="ump:UnoversalMediaPlayer"
		   x:Key="DefaultUnoversalMediaPlayerStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ump:UnoversalMediaPlayer">
                    <ContentPresenter Content="{TemplateBinding Content}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="ump:UnoversalMediaPlayer" BasedOn="{StaticResource DefaultUnoversalMediaPlayerStyle}" />
</ResourceDictionary>